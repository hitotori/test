package testgame.status;

public class SampleBraverStatus {
	public class Braver {
		int HP = 100;
		String name = "Braver";

		public int getHP() {
			return HP;
		}


		public void setHP(int hP) {
			HP = hP;
		}

		public String getName() {
			return name;
		}

		public void setName(String name) {
			this.name = name;
		}

	}
}
